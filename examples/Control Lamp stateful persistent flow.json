    {
        "id": "3adb060459b13f26",
        "type": "function",
        "z": "03f230443c464b5e",
        "name": "Extract submission values",
        "func": "var valuesArr = msg.savedSubmission.formComponents;\n\nvar lampSwitch = valuesArr.find(o => o.refName == msg.lampSwitchReference);\nvar lampDimmer = valuesArr.find(o => o.refName == msg.lampDimmerReference);\n\nmsg.sentValues = {};\n\nmsg.sentValues.lampSwitch = {\n    value: lampSwitch.value\n};\n\nmsg.sentValues.lampDimmer = {\n    value: lampDimmer.value\n};\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1070,
